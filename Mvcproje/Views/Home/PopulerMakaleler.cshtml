@model IEnumerable<Mvcproje.Models.Makale>

@{
    Layout = null;
}


<h5 class="card-header">En Çok Okunan Makaleler</h5>
<div class="card-body">
    <div class="col-lg-12">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.makaleid">@item.baslik</a> (@item.okunma)</li>
            }
        </ul>

    </div>


</div>